                    f({'typeTours': {}});
                }
            });
        }
    };

    o.hash.addTrigger(function(){
        if(otpMain.hash.get('type', '')) {
            otpMain.form.tourtype.view.$el.val(otpMain.hash.get('type', ''));
        }
    }, 40);

